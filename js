//task 1
"use strict";
var number= 100;
if (number < 50) {console.log ('less than 50 '); }
else if (number > 20) {console.log ('more than 20'); }
else {console.log ('error')}

//task 2
"use strict";
let name = 'mariami';
if (name ==  'mariami') {console.log ('true'); }
else {console.log ('false');}

//task4
'use strict'
let name = 'mariami'
switch(name){
    case 'mariami':
        console.log('true');
        break;
     default:
        console.log('false');
         break;
}
